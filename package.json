{
  "name": "rocketcode-framework-upgraded",
  "version": "1.0.0",
  "description": "A modern meta-framework built on Next.js and React with built-in TypeScript support, logging, and CLI tools",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./cli": {
      "import": "./dist/cli.mjs",
      "require": "./dist/cli.js",
      "types": "./dist/cli.d.ts"
    },
    "./logger": {
      "import": "./dist/logger.mjs",
      "require": "./dist/logger.js",
      "types": "./dist/logger.d.ts"
    },
    "./services": {
      "import": "./dist/services.mjs",
      "require": "./dist/services.js",
      "types": "./dist/services.d.ts"
    }
  },
  "bin": {
    "rocketcode": "./dist/cli.js",
    "create-rocketcode-app": "./dist/create-app.js"
  },
  "files": [
    "dist",
    "templates",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build",
    "test": "jest",
    "lint": "eslint src --ext .ts,.tsx,.js,.jsx",
    "format": "prettier --write src/**/*.{ts,tsx,js,jsx}"
  },
  "keywords": [
    "nextjs",
    "react",
    "typescript",
    "framework",
    "meta-framework",
    "cli",
    "logger",
    "ssr",
    "ssg"
  ],
  "author": "RocketCode Team",
  "license": "MIT",
  "dependencies": {
    "next": "^14.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "inquirer": "^9.2.12",
    "fs-extra": "^11.1.1",
    "glob": "^10.3.10",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "@types/node": "^20.8.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/fs-extra": "^11.0.1",
    "@types/inquirer": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.8.0",
    "@typescript-eslint/parser": "^6.8.0",
    "eslint": "^8.51.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.7.0",
    "prettier": "^3.0.3",
    "tsup": "^7.2.0",
    "typescript": "^5.2.0"
  },
  "peerDependencies": {
    "next": ">=13.0.0",
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
} 